<%=render 'layouts/admin_header'%>
 
<div class="row-fluid">
	<div class="outer_shadow span12">
		<div class="inner_shadow span12 cico">
	        
	        <!--index heading -->
	        <div class="row-fluid"> 
	          <h1><%=t("admin.users.index.title")%></h1>
	        </div>	
		    
		    <hr class="h_cool_line" />
		    
		   
		    
		    <!-- Command buttons --> 
		     <div class="row-fluid">
            	<div class="span12">
                	<div class="buttons_group">
    		        	<%=link_to t("admin.users.index.add"),
    		        	new_admin_user_path,
    		        	class:"btn btn-primary"%>
    	                
    	                <%=link_to t("admin.users.index.import"),'#',class:"btn btn-primary"%>
    	               
    		            <%=link_to t("admin.users.index.export"),'#',class:"btn btn-primary"%>
    		            
                    
                    </div>	    
                    
                    
       	
            	</div>         
            </div>
		    
		    <hr class="h_cool_line" /> 
		    
		   <%@users.each do |user| %> 
		     <%= render :partial=>'user',:locals=>{:user=>user} %>
		   <%end%> 
		  
		</div>
    </div>
</div>
        
